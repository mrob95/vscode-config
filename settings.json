{
    // Reduce gutter size
    "editor.glyphMargin": false,
    "editor.folding": true,
    "editor.lineDecorationsWidth": 0,
    // Scrollbar
    "editor.wordWrap": "on",
    // Never show horizontal
    "editor.scrollbar.horizontal": "hidden",
    // Always show vertical
    "editor.scrollbar.vertical": "visible",
    // Make thinner
    "editor.scrollbar.verticalScrollbarSize": 5,
    "editor.scrollbar.horizontalScrollbarSize": 0,
    // Visible colour
    "workbench.colorCustomizations": {
        "scrollbarSlider.background": "#bdae93",
        "[Solarized Light]": {
            "editorLineNumber.activeForeground": "#dc322f",
            "tab.border": "#dc322f",
            "tab.activeBorderTop": "#dc322f",
        },
        "[Gruvbox Dark (Hard)]": {
            // Doesn't work...
            // "editor.selectionBorder": "#83a598",
            // Defaults are way too dark,
            // and it's very hard to see what is selected
            // gruvbox colours:
            // https://github.com/morhetz/gruvbox
            "editor.selectionBackground": "#45858879",
            "editor.selectionHighlightBackground": "#d65e0e94",
            "editor.findMatchBorder": "#83a598",
            "editorCursor.foreground": "#ffffff",
            //
            "editorBracketMatch.background": "#d65e0e94",
            //
            "editorLineNumber.activeForeground": "#fabd2f",
            // "editorLineNumber.foreground": "#bdae93",
            //
            "editorGroupHeader.border": "#bdae93",
            "tab.border": "#bdae93",
            "tab.activeBorder": "#bdae93",
            "tab.activeBorderTop": "#bdae93",
            "tab.activeForeground": "#fbf1c7",
            "tab.inactiveForeground": "#ebdbb2",
        }
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "comment",
                    "comment.block",
                    "comment.block.documentation",
                    "comment.line"
                ],
                "settings": {
                    "fontStyle": ""
                }
            }
        ],
        "[Gruvbox Dark (Hard)]": {
            "comments": "#bdae93",
            "strings": "#8ec07c",
            "textMateRules": [
                {
                    "scope": "support.function.builtin.python",
                    "settings": {
                        "foreground": "#b8bb26"
                    }
                }
            ]
        }
    },
    "editor.cursorBlinking": "solid",
    "editor.cursorWidth": 3,
    // How is this in any way useful?
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    // "editor.overviewRulerBorder": false,
    "window.zoomLevel": 1,
    "workbench.activityBar.visible": false,
    "workbench.statusBar.visible": true,
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\cmd.exe",
    // "terminal.external.windowsExec": "C:/Program Files/Git/git-bash.exe",
    // Windows Terminal
    "terminal.external.windowsExec": "wt.exe",
    //
    "editor.emptySelectionClipboard": false,
    "editor.tabCompletion": "on",
    "editor.multiCursorModifier": "ctrlCmd",
    // The number of spaces a tab is equal to. This setting is overridden
    // based on the file contents when `editor.detectIndentation` is true.
    "editor.tabSize": 4,
    // Insert spaces when pressing Tab. This setting is overriden
    // based on the file contents when `editor.detectIndentation` is true.
    "editor.insertSpaces": true,
    // When opening a file, `editor.tabSize` and `editor.insertSpaces`
    // will be detected based on the file contents. Set to false to keep
    // the values you've explicitly set, above.
    "editor.detectIndentation": true,
    // Controls if quick suggestions should show up while typing
    "editor.quickSuggestions": true,
    // Don't open a million windows on start-up
    "window.restoreWindows": "one",
    "window.reopenFolders": "one",
    // Don't remember unsaved files
    "files.hotExit": "off",
    // Controls if suggestions should be accepted with "Enter" - in addition to "Tab". Helps to avoid ambiguity between inserting new lines and accepting suggestions.
    "editor.acceptSuggestionOnEnter": "off",
    "editor.suggestOnTriggerCharacters": true,
    "editor.hover.sticky": false,
    "workbench.editor.enablePreview": false,
    "workbench.startupEditor": "newUntitledFile",
    "gitHistory.showEditorTitleMenuBarIcons": false,
    "gitlens.hovers.enabled": false,
    "gitlens.menus": {
        "editor": {
            "blame": false,
            "clipboard": true,
            "compare": true,
            "details": false,
            "history": false,
            "remote": false
        },
        "editorGroup": false,
        "editorTab": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "explorer": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "scmGroup": {
            "compare": true,
            "openClose": true,
            "stash": true,
            "stashInline": true
        },
        "scmItem": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true,
            "stash": true
        }
    },
    "python.analysis.memory.keepLibraryAst": true,
    "python.analysis.memory.keepLibraryLocalVariables": true,
    "window.title": "${dirty}${rootName}${separator}${activeEditorLong}",
    "C_Cpp.updateChannel": "Insiders",
    "remote.SSH.showLoginTerminal": true,
    "python.formatting.provider": "black",
    "cmake.configureOnOpen": false,
    "files.trimTrailingWhitespace": true,
    "editor.largeFileOptimizations": true,
